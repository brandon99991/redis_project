<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cht.springboot.redis.cluster.mapper.GoodsMapper">
                                   
   <select id="findAll" resultType="com.cht.springboot.redis.cluster.model.Goods">
       select * from goods
   </select>
   
   <select id="findById" resultType="com.cht.springboot.redis.cluster.model.Goods">
      select * from goods where id=#{id}
   </select>
</mapper>